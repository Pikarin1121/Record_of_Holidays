<div class="container">
  <div class="row justify-content-center">
    <div class="col-lg-8 col-md-8 col-sm-8">

      <h4>通知一覧</h4>

      <% notifications = @notifications.where.not(visitor_id: current_user.id) %>
      <% if notifications.exists? %>
      <ul>
        <%= render notifications %>
      </ul>
      <% else %>
      <p>通知はありません</p>
      <% end %>

    </div>
  </div>
</div>